<div class="page-header">
  <%= link_to property_statuses_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_property_status_path(@property_status), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show property_status</h1>
</div>

<dl class="dl-horizontal">
  <dt>Code:</dt>
  <dd><%= @property_status.code %></dd>

  <dt>Name:</dt>
  <dd><%= @property_status.name %></dd>

</dl>
