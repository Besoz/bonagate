<div class="page-header">
  <%= link_to property_payment_plans_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_property_payment_plan_path(@property_payment_plan), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show property_payment_plan</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @property_payment_plan.name %></dd>

  <dt>Description:</dt>
  <dd><%= @property_payment_plan.description %></dd>

  <dt>Total value:</dt>
  <dd><%= @property_payment_plan.total_value %></dd>

  <dt>Property:</dt>
  <dd><%= @property_payment_plan.property %></dd>

</dl>
