<div class="page-header">
  <%= link_to property_states_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_property_state_path(@property_state), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show property_state</h1>
</div>

<dl class="dl-horizontal">
  <dt>Code:</dt>
  <dd><%= @property_state.code %></dd>

  <dt>Name:</dt>
  <dd><%= @property_state.name %></dd>

</dl>
